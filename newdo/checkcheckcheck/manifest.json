{
  "manifest_version": 2,

  "name": "Check Check Check!",
  "description": "To-do list in you new tabs. Synced in Chrome across all your computers.",
  "version": "0.0.0.4",
  
  "icons": {
            "16": "newdoIcon16.png",
            "128": "newdoIcon128.png"
             },
  
  "chrome_url_overrides" : {
      "newtab": "newdo.html"
  },

  
  "content_security_policy": "script-src 'self' https://fonts.googleapis.com/; object-src 'self'",
  
  "permissions": ["storage"]
}
