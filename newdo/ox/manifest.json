{
  "manifest_version": 2,

  "name": "NewDo",
  "description": "Sync a ToDo list across all your new tabs, across all your computers.",
  "version": "0.001",
  
  "icons": {
            "16": "ox16.png",
            "128": "ox128.png"
             },
  
  "chrome_url_overrides" : {
      "newtab": "ox.html"
  },

  
  "content_security_policy": "script-src 'self'; object-src 'self'",
  
  "permissions": ["storage"]
}
